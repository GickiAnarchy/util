#:kivy 1.0.0
#filename: yt.kv


<SearchScreen>:
    txt_in:txt_in
    
    cols:1
    size_hint:None,None
    size:275,400
    background_color:0,1,0,1

    Label:
        text:"Search YouTube"
        font_size:32
        size_hint:None,None
        size:250,45
        top: root.height - 5
    TextInput:
        id:txt_in
        text:"fatheranarchy"
        size_hint:None,None
        size:250,30
    Spinner:
        id:res_spin
        values:root.res_list
    Button:
        text:"Search"
        size_hint:None,None
        size:250,30
        on_press: root.do_search()